{
  "name": "gulp-ngtemplateurl",
  "version": "0.0.5",
  "description": "Simple gulp plugin to replace AngularJS templateUrl with template ",
  "main": "./dist/index.js",
  "scripts": {
    "prepublish": "npm build",
    "start": "nodemon lib/index.js --exec babel-node",
    "test": "mocha --compilers js:babel-register",
    "build": "babel-cli lib -d dist"
  },
  "keywords": [
    "gulp",
    "gulp-angular",
    "template",
    "template-url",
    "gulp-ng"
  ],
  "author": "Igor Santana <igorpsant95@gmail.com> (igorpsantana.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.4.3",
    "gulp": "^3.9.0",
    "mocha": "^2.3.4",
    "should": "^8.1.1"
  },
  "dependencies": {
    "readable-stream": "^2.0.5"
  }
}
